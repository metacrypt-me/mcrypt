#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/j/Projects/JS/Svelte/mcrypt/node_modules/.pnpm/typescript@4.4.4/node_modules/typescript/bin/node_modules:/home/j/Projects/JS/Svelte/mcrypt/node_modules/.pnpm/typescript@4.4.4/node_modules/typescript/node_modules:/home/j/Projects/JS/Svelte/mcrypt/node_modules/.pnpm/typescript@4.4.4/node_modules:/home/j/Projects/JS/Svelte/mcrypt/node_modules/.pnpm/node_modules:/home/j/Projects/JS/Svelte/mcrypt/node_modules:/home/j/Projects/JS/Svelte/node_modules:/home/j/Projects/JS/node_modules:/home/j/Projects/node_modules:/home/j/node_modules:/home/node_modules:/node_modules:/home/j/Projects/JS/Svelte/mcrypt/node_modules/.pnpm/@typescript-eslint+eslint-plugin@4.33.0_cc617358c89d3f38c52462f6d809db4c/node_modules/@typescript-eslint/eslint-plugin/node_modules:/home/j/Projects/JS/Svelte/mcrypt/node_modules/.pnpm/@typescript-eslint+eslint-plugin@4.33.0_cc617358c89d3f38c52462f6d809db4c/node_modules/@typescript-eslint/node_modules:/home/j/Projects/JS/Svelte/mcrypt/node_modules/.pnpm/@typescript-eslint+eslint-plugin@4.33.0_cc617358c89d3f38c52462f6d809db4c/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/j/Projects/JS/Svelte/mcrypt/node_modules/.pnpm/typescript@4.4.4/node_modules/typescript/bin/node_modules:/home/j/Projects/JS/Svelte/mcrypt/node_modules/.pnpm/typescript@4.4.4/node_modules/typescript/node_modules:/home/j/Projects/JS/Svelte/mcrypt/node_modules/.pnpm/typescript@4.4.4/node_modules:/home/j/Projects/JS/Svelte/mcrypt/node_modules/.pnpm/node_modules:/home/j/Projects/JS/Svelte/mcrypt/node_modules:/home/j/Projects/JS/Svelte/node_modules:/home/j/Projects/JS/node_modules:/home/j/Projects/node_modules:/home/j/node_modules:/home/node_modules:/node_modules:/home/j/Projects/JS/Svelte/mcrypt/node_modules/.pnpm/@typescript-eslint+eslint-plugin@4.33.0_cc617358c89d3f38c52462f6d809db4c/node_modules/@typescript-eslint/eslint-plugin/node_modules:/home/j/Projects/JS/Svelte/mcrypt/node_modules/.pnpm/@typescript-eslint+eslint-plugin@4.33.0_cc617358c89d3f38c52462f6d809db4c/node_modules/@typescript-eslint/node_modules:/home/j/Projects/JS/Svelte/mcrypt/node_modules/.pnpm/@typescript-eslint+eslint-plugin@4.33.0_cc617358c89d3f38c52462f6d809db4c/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../typescript@4.4.4/node_modules/typescript/bin/tsserver" "$@"
else
  exec node  "$basedir/../../../../../../typescript@4.4.4/node_modules/typescript/bin/tsserver" "$@"
fi
