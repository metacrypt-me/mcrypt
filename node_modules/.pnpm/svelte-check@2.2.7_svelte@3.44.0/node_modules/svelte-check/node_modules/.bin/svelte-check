#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/j/Projects/JS/Svelte/mcrypt/node_modules/.pnpm/svelte-check@2.2.7_svelte@3.44.0/node_modules/svelte-check/bin/node_modules:/home/j/Projects/JS/Svelte/mcrypt/node_modules/.pnpm/svelte-check@2.2.7_svelte@3.44.0/node_modules/svelte-check/node_modules:/home/j/Projects/JS/Svelte/mcrypt/node_modules/.pnpm/svelte-check@2.2.7_svelte@3.44.0/node_modules:/home/j/Projects/JS/Svelte/mcrypt/node_modules/.pnpm/node_modules:/home/j/Projects/JS/Svelte/mcrypt/node_modules:/home/j/Projects/JS/Svelte/node_modules:/home/j/Projects/JS/node_modules:/home/j/Projects/node_modules:/home/j/node_modules:/home/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/j/Projects/JS/Svelte/mcrypt/node_modules/.pnpm/svelte-check@2.2.7_svelte@3.44.0/node_modules/svelte-check/bin/node_modules:/home/j/Projects/JS/Svelte/mcrypt/node_modules/.pnpm/svelte-check@2.2.7_svelte@3.44.0/node_modules/svelte-check/node_modules:/home/j/Projects/JS/Svelte/mcrypt/node_modules/.pnpm/svelte-check@2.2.7_svelte@3.44.0/node_modules:/home/j/Projects/JS/Svelte/mcrypt/node_modules/.pnpm/node_modules:/home/j/Projects/JS/Svelte/mcrypt/node_modules:/home/j/Projects/JS/Svelte/node_modules:/home/j/Projects/JS/node_modules:/home/j/Projects/node_modules:/home/j/node_modules:/home/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/svelte-check" "$@"
else
  exec node  "$basedir/../../bin/svelte-check" "$@"
fi
