#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/j/Projects/JS/Svelte/mcrypt/node_modules/.pnpm/nanoid@3.1.30/node_modules/nanoid/bin/node_modules:/home/j/Projects/JS/Svelte/mcrypt/node_modules/.pnpm/nanoid@3.1.30/node_modules/nanoid/node_modules:/home/j/Projects/JS/Svelte/mcrypt/node_modules/.pnpm/nanoid@3.1.30/node_modules:/home/j/Projects/JS/Svelte/mcrypt/node_modules/.pnpm/node_modules:/home/j/Projects/JS/Svelte/mcrypt/node_modules:/home/j/Projects/JS/Svelte/node_modules:/home/j/Projects/JS/node_modules:/home/j/Projects/node_modules:/home/j/node_modules:/home/node_modules:/node_modules:/home/j/Projects/JS/Svelte/mcrypt/node_modules/.pnpm/postcss@8.3.11/node_modules/postcss/node_modules:/home/j/Projects/JS/Svelte/mcrypt/node_modules/.pnpm/postcss@8.3.11/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/j/Projects/JS/Svelte/mcrypt/node_modules/.pnpm/nanoid@3.1.30/node_modules/nanoid/bin/node_modules:/home/j/Projects/JS/Svelte/mcrypt/node_modules/.pnpm/nanoid@3.1.30/node_modules/nanoid/node_modules:/home/j/Projects/JS/Svelte/mcrypt/node_modules/.pnpm/nanoid@3.1.30/node_modules:/home/j/Projects/JS/Svelte/mcrypt/node_modules/.pnpm/node_modules:/home/j/Projects/JS/Svelte/mcrypt/node_modules:/home/j/Projects/JS/Svelte/node_modules:/home/j/Projects/JS/node_modules:/home/j/Projects/node_modules:/home/j/node_modules:/home/node_modules:/node_modules:/home/j/Projects/JS/Svelte/mcrypt/node_modules/.pnpm/postcss@8.3.11/node_modules/postcss/node_modules:/home/j/Projects/JS/Svelte/mcrypt/node_modules/.pnpm/postcss@8.3.11/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../nanoid@3.1.30/node_modules/nanoid/bin/nanoid.cjs" "$@"
else
  exec node  "$basedir/../../../../../nanoid@3.1.30/node_modules/nanoid/bin/nanoid.cjs" "$@"
fi
